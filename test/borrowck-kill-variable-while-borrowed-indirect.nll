// Corresponds to:
//
// ```
// let q;
// {
//   let foo = Vec { dummy: () };
//   {
//     let p = &foo;
//     q = &*p;
//   }
// }
// print(q);
// ```
//
// Should be error, since `q` forces borrow of `p` to outlive `foo`.

struct Vec<+> {
  field: 0
}  

let foo: Vec<()>;
let p: &'_ Vec<()>;
let q: &'_ Vec<()>;

block START {
    foo = use();
    p = &'_ foo;
    q = &'_ *p;
    StorageDead(p);
    StorageDead(foo); //! `foo` is borrowed
    use(q);
    StorageDead(q);
}

